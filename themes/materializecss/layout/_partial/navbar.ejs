<div class="navbar-fixed">
  <nav>
    <div class="nav-wrapper teal darken-1">
      <div class="container">
        <a href="#!" class="brand-logo">
          <%= config.title %>
        </a>
        <a href="#" data-target="mobileNav" class="sidenav-trigger"><i class="material-icons">menu</i></a>
        <ul class="right hide-on-med-and-down">
          <% for (var i in theme.menu){ %>
            <li>
              <a class="waves-effect waves-light" href="<%- url_for(theme.menu[i]) %>">
                <%= i %>
              </a>
            </li>
            <% } %>
        </ul>
      </div>
    </div>
  </nav>
</div>

<ul class="sidenav" id="mobileNav">
  <% for (var i in theme.menu){ %>
    <li>
      <a class="waves-effect waves-green" href="<%- url_for(theme.menu[i]) %>">
        <%= i %>
      </a>
    </li>
    <% } %>
</ul>
